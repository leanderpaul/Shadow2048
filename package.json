{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "main": "public/main.js",
    "dependencies": {
        "arrow-keys-react": "^1.0.6",
        "electron-is-dev": "^1.1.0",
        "mdbreact": "^4.13.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-keydown": "^1.9.8",
        "react-scripts": "3.0.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
